{
  "name": "pelias-interpolation",
  "version": "0.0.0-semantic-release",
  "description": "street address interpolation service",
  "homepage": "https://github.com/pelias/api",
  "author": "mapzen",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "npm run units",
    "funcs": "MOCK_LIBPOSTAL=true node test/_func.js | tap-spec",
    "units": "MOCK_LIBPOSTAL=true node test/_unit.js | tap-spec",
    "travis": "npm run check-dependencies && npm test && npm run funcs",
    "lint": "jshint .",
    "validate": "npm ls",
    "check-dependencies": "node_modules/.bin/npm-check --production --ignore node-postal",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pelias/interpolation.git"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "cli-table2": "^0.2.0",
    "csv-parse": "^1.2.0",
    "express": "^4.14.1",
    "node-postal": "^0.3.0",
    "@mapbox/polyline": "^0.2.0",
    "quadtree": "^1.1.3",
    "require-dir": "^0.3.1",
    "serve-index": "^1.8.0",
    "split2": "^2.1.1",
    "sqlite3": "^3.1.7",
    "through2": "^2.0.3",
    "through2-batch": "^1.0.1"
  },
  "devDependencies": {
    "jshint": "^2.9.3",
    "lodash": "^4.17.4",
    "npm-check": "git://github.com/orangejulius/npm-check.git#disable-update-check",
    "precommit-hook": "^3.0.0",
    "proxyquire": "^1.7.11",
    "semantic-release": "^6.3.2",
    "tap-dot": "^1.0.5",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  },
  "pre-commit": [
    "lint",
    "validate",
    "test",
    "check-dependencies"
  ]
}
